 from \"@/types/Device\";\nimport ResponseData from \"@/types/ResponseData\";\n\nexport default defineComponent({\n  name: \"add-device\",\n  data() {\n    return {\n      device: {\n        id: null,\n        title: \"\",\n        description: \"\",\n        published: false,\n      } as Device,\n      submitted: false,\n    };\n  },\n  methods: {\n    saveDevice() {\n      let data = {\n        title: this.device.title,\n        description: this.device.description,\n      };\n\n      DeviceDataService.create(data)\n        .then((response: ResponseData) => {\n          this.device.id = response.data.id;\n          console.log(response.data);\n          this.submitted = true;\n        })\n        .catch((e: Error) => {\n          console.log(e);\n        });\n    },\n\n    newDevice() {\n      this.submitted = false;\n      this.tutorial = {} as Device;\n    },\n  },\n});\n</script>\n\n<style>\n.submit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"]}]}