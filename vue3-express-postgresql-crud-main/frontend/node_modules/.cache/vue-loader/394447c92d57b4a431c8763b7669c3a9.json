  :hide-default-footer=\"true\"\n        >\n          <template v-slot:[`item.actions`]=\"{ item }\">\n            <v-icon small class=\"mr-2\" @click=\"editDevice(item.id)\">mdi-pencil</v-icon>\n            <v-icon small @click=\"deleteDevice(item.id)\">mdi-delete</v-icon>\n          </template>\n        </v-data-table>\n\n        <v-card-actions v-if=\"devices.length > 0\">\n          <v-btn small color=\"error\" @click=\"removeAllDevices\">\n            Remove All\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\nimport DeviceDataService from \"../services/DeviceDataService\";\nexport default {\n  name: \"devices-list\",\n  data() {\n    return {\n      devices: [],\n      title: \"\",\n      headers: [\n        { text: \"Title\", align: \"start\", sortable: false, value: \"title\" },\n        { text: \"Description\", value: \"description\", sortable: false },\n        { text: \"Price\", value: \"price\", sortable: false },\n        { text: \"Brand\", value: \"brand\", sortable: false },\n        { text: \"Condition\", value: \"condition\", sortable: false },\n        { text: \"Available\", value: \"available\", sortable: false },\n        { text: \"In stock\", value: \"in_stock\", sortable: false },\n        { text: \"Warranty\", value: \"warranty\", sortable: false },\n        { text: \"Delivery options\", value: \"delivery_options\", sortable: false },\n        { text: \"Status\", value: \"status\", sortable: false },\n        { text: \"Actions\", value: \"actions\", sortable: false },\n      ],\n    };\n  },\n  methods: {\n    retrieveDevices() {\n      DeviceDataService.getAll()\n        .then((response) => {\n          this.devices = response.data.map(this.getDisplayDevice);\n          console.log(response.data);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    refreshList() {\n      this.retrieveDevices();\n    },\n\n    removeAllDevices() {\n      DeviceDataService.deleteAll()\n        .then((response) => {\n          console.log(response.data);\n          this.refreshList();\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    searchTitle() {\n      DevicelDataService.findByTitle(this.title)\n        .then((response) => {\n          this.devices = response.data.map(this.getDisplayDevice);\n          console.log(response.data);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    editDevice(id) {\n      this.$router.push({ name: \"device-details\", params: { id: id } });\n    },\n\n    deleteDevice(id) {\n      DeviceDataService.delete(id)\n        .then(() => {\n          this.refreshList();\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    getDisplayDevice(device) {\n      return {\n        id: device.id,\n        title: device.title.length > 30 ? device.title.substr(0, 30) + \"...\" : device.title,\n        description: device.description.length > 30 ? device.description.substr(0, 30) + \"...\" : device.description,\n        price: device.price.length > 30 ? device.price.substr(0, 30) + \"...\" : device.price,\n        brand: device.brand.length > 30 ? device.brand.substr(0, 30) + \"...\" : device.brand,\n        condition: device.condition.length > 30 ? device.condition.substr(0, 30) + \"...\" : device.condition,\n        available: device.available.length > 30 ? device.available.substr(0, 30) + \"...\" : device.available,\n        in_stock: device.in_stock.length > 30 ? device.in_stock.substr(0, 30) + \"...\" : device.in_stock,\n        warranty: device.warranty.length > 30 ? device.warranty.substr(0, 30) + \"...\" : device.warranty,\n        delivery_options: device.delivery_options.length > 30 ? device.delivery_options.substr(0, 30) + \"...\" : device.delivery_options,\n        status: device.published ? \"Published\" : \"Pending\",\n      };\n    },\n  },\n  mounted() {\n    this.retrieveDevices();\n  },\n};\n</script>\n\n<style>\n.list {\n  max-width: 750px;\n}\n</style>"]}]}